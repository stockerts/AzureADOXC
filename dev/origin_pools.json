{
  "metadata": {
    "name": "<name>",
    "labels": {}
  },
  "spec": {
    "origin_servers": [
      {
        "public_name": {
          "dns_name": "<fqdn>"
        },
        "labels": {}
      }
    ],
    "use_tls": {
      "use_host_header_as_sni": {},
      "tls_config": {
        "default_security": {}
      },
      "volterra_trusted_ca": {},
      "no_mtls": {}
    },
    "port": 443,
    "same_as_endpoint_port": {},
    "healthcheck": [
      {
        "tenant": "<tenant>",
        "namespace": "<namespace>",
        "name": "<healthcheckname>",
        "kind": "healthcheck"
      }
    ],
    "loadbalancer_algorithm": "LB_OVERRIDE",
    "endpoint_selection": "LOCAL_PREFERRED"
  }
}